{
  "name": "Music Artwork Downloader",
  "description": "Let's you easily download artboards from http://music.apple.com",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    }
  },
  "permissions": ["activeTab"]
}
